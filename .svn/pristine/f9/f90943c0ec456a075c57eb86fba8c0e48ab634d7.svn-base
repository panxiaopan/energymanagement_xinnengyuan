              shadowColor: "#05C3F9"
            }
          },
          zlevel: 1
        }
      ],
      config: {
        geo: {
          // map: "china",
          // label: {
          //   emphasis: {
          //     show: false
          //   }
          // },
          // itemStyle: {
          //   normal: {
          //     borderColor: "#006bfa",
          //     areaColor: "#100072"
          //   },
          //   emphasis: {
          //     areaColor: "#006bfa"
          //   }
          // }
        }
      },
      baseUrl,
      keyword: "",
      orderCol: "",
      orderType: "desc",
      start: 0,
      size: 3,
      totalnum: 0,
      typeoptions: [
        {
          value: "installedDate",
          label: "安装日期"
        },
        {
          value: "currentStatus",
          label: "当前状态"
        },
        {
          value: "deviceCount",
          label: "设备数量"
        }
      ],
      TabactiveName: "first",
      nowtime: timeFormanow(new Date()),
      timeFormanow,
      zoom: 9,
      mapStyle: {
        styleJson: [
          {
