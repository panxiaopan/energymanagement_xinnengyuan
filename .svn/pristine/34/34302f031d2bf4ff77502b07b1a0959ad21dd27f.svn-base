 
<template>
  <el-row>
    <el-container>
      <el-header style="height:62px">
        <div class="header_logo">
          
           <img :src="loginLogoUrl"     :style="dynamicstate">

           <!-- <img src="static/imgs/logo_img.png" style="margin-top:15px;" alt srcset   v-else  /> -->
        </div>
        <el-dropdown @command="handleCommand" class="usenameshow">
          <!-- <i class="el-icon-setting" style="margin-right: 15px"></i> -->
          <el-button icon="el-icon-user-solid" size="small">{{loginName}}</el-button>
          <el-dropdown-menu slot="dropdown">
            <el-dropdown-item command="退出">退出</el-dropdown-item>
          </el-dropdown-menu>
        </el-dropdown>
        <div>
          <el-menu
            :default-active="$route.path"
            class="el-menu-demo"
            mode="horizontal"
            @select="handleSelect"
            :unique-opened="true"
            active-text-color="#ffd04b"
          >
            <template v-for=" (item,index) in menudata">
              <el-menu-item :key="item.menudata" :index="item.path" v-if="item.subNodes.length ==0    ">
                <template slot="title">
                  <i :class="item.icon" class="iconfont"></i>
                  <span class="menu_Name"   >{{item.name}}</span>
                </template>
              </el-menu-item>
              <el-submenu :key="item.index" :index="String(index)" v-if="item.subNodes.length !==0">
                <template slot="title">
                  <i :class="item.icon" class="iconfont"></i>
                  <span class="menu_Name">{{item.name}}</span>
                </template>
                <el-menu-item :key="item.index" :index="item.path" v-for="item in item.subNodes">
                  <!-- <i :class="item.icon" class="iconfont"></i> -->
                  {{item.name}}
                </el-menu-item>
              </el-submenu>
            </template>
          </el-menu>
        </div>
      </el-header>

      <el-container>
        <el-aside width="270px">
          <el-col>
            <div class="back_main" @click="backMain">
              <i class="iconfont icon-back" style="margin-right:10px"></i>
              <span class="StionName">{{name}}</span>
            </div>
            <el-menu
              class="el-menu-vertical-demo"
              :default-active="$route.path"
              @select="selectchirden"
              :default-openeds="opens"
            >
              <template v-for=" (item,index) in submenuData">
                <el-menu-item
                  :key="index"
                  :index="item.path"
                  v-if="item.submenuDatachirden.length==0 && item.path!=='/' && $route.query.realTime!=1 "
                >
                  <template slot="title">
                    <!-- <i  :class="item[0].permissionIcon" class="fa  menuicon"></i> -->
                    <span>{{item.name}}</span>
                  </template>
                </el-menu-item>
                <el-submenu
                  :key="index"
                  :index="String(index)"
                  v-if="item.submenuDatachirden.length !==0"
                >
                  <template slot="title">
                    <!-- <i  :class="item[0].permissionIcon" class="fa"></i>   -->

                    <span>{{item.name}}</span>
                  </template>

                  <el-menu-item
                    :key="item.index"
                    :index="item.path"
                    v-for="item in item.submenuDatachirden"
                  >
                    <i :class="item.img" class="iconfont"></i>
                    <!-- <img :src="item.img" alt class="subimgs" /> -->
                    {{item.name}}
                  </el-menu-item>
                </el-submenu>
              </template>
            </el-menu>
          </el-col>
        </el-aside>
        <el-main>
          <router-view></router-view>
        </el-main>
      </el-container>
      <!-- <el-footer>Footer</el-footer> -->
    </el-container>
  </el-row>
</template>

<script>
import {
  getstationsidNameType,
  getstationssummary,
  sessionlogout
} from "@/api/api";
export default {
  data() {
    return {
      menudata: [],
      // activeIndex2: "1"
      name: "",
      submenuData: [
        {
          name: "能源综合总汇",
          path: `/collect/${this.$route.params.id}`,
          submenuDatachirden: []
        },
        {
          name: "子站列表",
          path: "/",
          submenuDatachirden: []
        }
      ], //子站菜单
      loginName: null,
      opens: ["0", "1", "2"],
      customLoginInfo:{},
      loginLogoUrl:"",
      dynamicstate:{},
    };
  },
  methods: {
    getname() {
      getstationssummary(this.$route.params.id).then(res => {
        // console.log("name-------------------------");
        // console.log(res);
        if (res.data.head.code == 0) {
          // console.log("name-------------------------");
          // console.log(res);
          this.name = res.data.data.stationConfigOverview.name;
        }
      });
    },
    handleSelect(index) {
      //跳转路由
      console.log(index);
      if (index == "/mapPages") {
        this.$router.push("/mainmap");
      } else {
        this.$router.push(index);
      }
    },
    handleCommand(command) {
      //退出
      console.log(command);
      if (command == "退出") {
        window.sessionStorage.removeItem("menudata"); //退出时候就清空
        sessionlogout().then(res => {
          this.$router.push("/login");
        });
      }
    },
    getmenu() {
      var parms = this.$route.params.id;
      getstationsidNameType(parms).then(res => {
        if (res.data.head.code == 0) {
          console.log("caidan");
          console.log(res);
          let menu = res.data.data;
          if (menu.length > 0) {
            for (let i = 0; i < menu.length; i++) {
              if (menu[i].type.value === 10) {
                this.submenuData[1].submenuDatachirden.push({
                  name: "光伏",
                  path: `/Photovoltaic/${this.$route.params.id}/${menu[i].id}`,
                  img: "icon-guangfuban"
                });
              } else if (menu[i].type.value === 40) {
                this.submenuData[1].submenuDatachirden.push({
                  name: "消防",
                  path: `/firecontrol/${this.$route.params.id}/${menu[i].id}`,
                  img: "icon-firehydrant1-device"
                });
              } else if (menu[i].type.value === 20) {
                this.submenuData[1].submenuDatachirden.push({
                  name: "储能",
                  path: `/storedenery/${this.$route.params.id}/${menu[i].id}`,
                  img: "icon-chunengguanli"
                });
              } else if (menu[i].type.value === 50) {
                this.submenuData[1].submenuDatachirden.push({
                  name: "安防",
                  path: `/safeprotection/${this.$route.params.id}/${menu[i].id}`,
                  img: "icon-shexiangtou"
                });
              } else if (menu[i].type.value === 30) {
                this.submenuData[1].submenuDatachirden.push({
                  name: "充电站",
                  path: `/chargestation/${this.$route.params.id}/${menu[i].id}`,
                  img: "icon-chongdianzhan"
                });
              } else if (menu[i].type.value === 60) {
                this.submenuData[1].submenuDatachirden.push({
                  name: "能耗",
                  img: "icon-nenghaoguanli",
                  path: `/switchroom/${this.$route.params.id}/${menu[i].id}`
                });
              }
            }
          }
          //console.log("00菜单-----");
          // console.log(datamenu);
        }
      });
    },
    backMain() {
      //返回上一页
      //this.$router.go(-1);
      this.$router.push("/realTimeMain");
    },
    selectchirden(index) {
      console.log(index);
      // console.log(keyPath);
      this.$router.push(index);
    }
  },
  mounted() {
    //             console.log('登录信息=========')
    //  console.log( this.customLoginInfo)
           this.customLoginInfo = JSON.parse(
          sessionStorage.getItem("_HUAJE_customLoginInfo_before")
        );
        if(this.customLoginInfo ){
              let data = this.customLoginInfo
              let groupId = data.groupId;
              let picName =  data.logoDefaultFile;
          this.loginLogoUrl = `./files/filestream?type=ienergy-logo&id=${groupId}&filename=${picName}`;
            this.$nextTick(()=>{
             this.dynamicstate={
                   display:'inline-block',
                   'vertical-align': 'middle',
                   marginTop:data.data.logoDefaultFileTop+'px',
                   marginLeft:data.data.logoDefaultFileLeft+"px",
                   width:data.data.logoDefaultFileWidth+'px',
                   height:data.data.logoDefaultFileHeight+'px'
             }
        })



        }
    this.getname(); //huoqm
    this.menudata = JSON.parse(sessionStorage.getItem("menudata"));

    console.log(this.menudata);
    this.getmenu();
    this.loginName = sessionStorage.getItem("loginName");
    console.log("新的路由");
    console.log(this.$route);

    if (
      this.$route.name == "能源汇总" ||
      this.$route.name == "光伏" ||
      this.$route.name == "储能" ||
      this.$route.name == "充电站" ||
      this.$route.name == "消防" ||
      this.$route.name == "安防" ||
      this.$route.name == "能耗"
    ) {
      console.log("进来了");
      this.$nextTick(() => {
        setTimeout(() => {
          let chirdenodes = $(".el-menu-demo")[0].children[1];
          $(chirdenodes).addClass("is__active");
          let nodesico = $(chirdenodes)[0].children;
          // console.log("=====dwdwdwdw");
          // console.log(nodesico);
          $(nodesico).addClass("is__active");
          // console.log(nodesico)
        }, 1000);
        //if()
      });
      // $(chirdenodes[1]).addClass("intro");
      // console.log($(chirdenodes[1]));
      //console.log(chirdenodes[0]);
    }







  }
};
</script>

<style lang="scss" scoped>
.is__active {
  color: rgb(5, 40, 255);
}
.el-main {
  height: 100%;
}
.el-row {
  height: calc(100% - 8px);
  // margin-bottom: 20px;
  &:last-child {
    margin-bottom: 0;
  }
}
.bg-purple {
  background: #d3dce6;
}
.bg-purple-light {
  background: #e5e9f2;
}
.grid-content {
  border-radius: 4px;
  min-height: 36px;
}
.el-menu-vertical-demo {
  // border: 1px solid #e6e6e6;
}
.back_main {
  height: 60px;
  // border: 1px solid #e6e6e6;
  border-bottom: none;
  background: #f4f4fa;
  text-align: center;
  line-height: 60px;
  cursor: pointer;
}
.unitName {
  font-size: 14px;
  color: #636f8a;
  margin-left: 10px;
  display: inline-block;
}
.subimgs {
  // width: 40px;
  // height: 40px;
  margin-right: 5px;
}
.StionName {
  font-size: 14px;
  font-family: Microsoft YaHei;
  font-weight: 400;
}
</style>
