   //获取年月
   export const timeFormatmonth = (value) => {
     let date = new Date(value)
     var y = date.getFullYear();
     var MM = date.getMonth() + 1;
     MM = MM < 10 ? ('0' + MM) : MM
     var d = date.getDate();
     d = d < 10 ? ('0' + d) : d
     var h = date.getHours();
     h = h < 10 ? ('0' + h) : h
     var m = date.getMinutes()
     m = m > 9 ? m : ('0' + m)
     var ss = date.getSeconds()
     ss = ss > 9 ? ss : ('0' + ss)
     return y + '-' + MM
   }

   //获取当前月的上一个月
   export const timePrveFormatmonth = (value) => {
     let date = new Date(value)
     var y = date.getFullYear();
     var MM = date.getMonth() - 2;
     MM = MM < 10 ? ('0' + MM) : MM
     return y + '-' + MM
   }








   //获取年
   export const timeFormatyear = (value) => {
