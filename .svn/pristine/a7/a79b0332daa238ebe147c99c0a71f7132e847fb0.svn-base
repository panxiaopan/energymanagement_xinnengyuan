.hj-inspection-plan-wrapper {
  background: #ffffff;
  padding: 1vh 1vw;
  border-radius: 8px;
  .hj-inspection-plan__head {
    background: #ffffff;
    margin-bottom: 1vh;
    padding: 1vh 0.5vw;
    .panel__item {
      display: flex;
      align-items: center;
      padding: 0.5vh 0;
    }
    .panel__item__left {
      margin-right: 1.67vw;
      font-size: 1.48vh;
      font-weight: bold;
      width: 3.9vw;
      text-align: justify;
      overflow: hidden;
      vertical-align: top;
      height: 2.5vh;
    }
    .panel__item__left ::after {
      content: " ";
      display: inline-block;
      width: 100%;
      height: 0;
    }
    .panel__item__right {
      display: flex;
      align-items: center;
    }
    .panel__item__right__more {
      padding-left: 1vw;
      color: #20a0ff;
      cursor: pointer;
    }
    .panel__item__right--report {
      // display: flex;
      // justify-content: flex-end;
      position: absolute;
      right: 0.5vw;
    }
  }

  .hj-inspection-plan__pagination {
    display: flex;
    justify-content: flex-end;
    padding-top: 1vh;
  }
}
